<!--index.wxml-->
<view class="container">
  <view class="result-logo-div">
    <image class="result-logo" mode="aspectFit" src="/img/logo.png"></image>
  </view>

  <view class="result-title">
    <text>{{title}}</text>
  </view>

  <view class="result-msg">
    <text decode="true">{{resultMsg}}</text>
  </view>

  <view class="jump-btn-div">
    <button class="jump-btn" plain="true" bindtap="jump">{{buttonText}}</button>
  </view>
</view>
